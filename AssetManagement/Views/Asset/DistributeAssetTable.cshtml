@model IEnumerable<AssetManagement.Models.ViewModel.DistributeAssetViewModel>
<table class="table table-striped table-bordered table-hover table-checkable order-column" id="sample_1">
    <thead>
        <tr>
            <th>
                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                    <input type="checkbox" class="group-checkable" data-set="#sample_1 .checkboxes" />
                    <span></span>
                </label>
            </th>
            <th> ID </th>
            <th> Name </th>
            <th> Tag </th>
            <th> Store</th>
        </tr>
    </thead>
    <tbody id="check_box_body">
        @foreach (var asset in Model)
        {
        <tr class="odd gradeX">
            <td>
                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                    <input type="checkbox" class="checkboxes" value="@asset.AssetID" />
                    <span></span>
                </label>
            </td>
            <td> @asset.AssetID </td>
            <td> @asset.AssetName </td>
            <td> @asset.AssetTag </td>
            <td> @asset.StoreName </td>
        </tr>
        }
    </tbody>
</table>
